#include <zephyr/dt-bindings/gpio/sixtron-header.h>

&sixtron_i2c {
	status = "okay";

	max172010: max17201@6C {
		compatible = "maxim,max17201";
		reg = <0x6C>;
		alert-gpios = <&sixtron_connector DIO1 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>;
	};
};
